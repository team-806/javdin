// Example 2.1: Constant Folding
// This demonstrates constant expression simplification

var result1 := 2 + 3
var result2 := 10 * 5
var result3 := 100 / 4
var result4 := 20 - 7

var flag1 := true and false
var flag2 := true or false

var comparison1 := 5 < 10
var comparison2 := 15 = 15

print result1
print result2
print result3
print result4
print flag1
print flag2
print comparison1
print comparison2
